#!/bin/bash 
# DO NOT EDIT THIS FILE

size=${#wordarray[@]}

rand=$(( ( RANDOM % size )  + 1 ))

RED='\033[31m'
PURPLE='\033[0;35m'
NC='\033[0m' # No Color
bold=$(tput bold)
normal=$(tput sgr0)
printf "\n"
printf "${bold}Word Of The Execution: ${RED}${wordarray[rand-1],,}${NC}\n"
printf "${bold}Meaning              : ${PURPLE}${meaningarray[rand-1],,}${NC}\n"
printf "\n"

exit 0
